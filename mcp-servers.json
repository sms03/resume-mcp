{
  "mcpServers": {
    "resume-analysis": {
      "command": "python",
      "args": ["adk_main.py"],
      "cwd": "d:\\GitHub\\resume-mcp",
      "env": {
        "WEB_UI_ENABLED": "true",
        "MCP_TRANSPORT": "http",
        "SERVER_HOST": "localhost",
        "SERVER_PORT": "3000"
      },
      "name": "Resume Analysis MCP Server",
      "description": "Intelligent resume analysis and candidate selection using Google Agent Development Kit",
      "version": "1.0.0",
      "author": "Resume Analysis Team",
      "capabilities": {
        "tools": [
          "analyze_resume",
          "batch_analyze_resumes", 
          "score_resume",
          "sort_resumes",
          "filter_resumes",
          "match_job",
          "extract_skills",
          "generate_report"
        ],
        "transport": ["http", "stdio"],
        "web_ui": true
      },
      "endpoints": {
        "base_url": "http://localhost:3000",
        "health": "/health",
        "status": "/",
        "analyze": "/analyze-resume",
        "batch": "/batch-analyze",
        "score": "/score-resume",
        "upload": "/upload-resume"
      },
      "metadata": {
        "category": "analysis",
        "tags": ["resume", "ai", "hr", "recruitment", "analysis"],
        "adk_compatible": true,
        "web_server": true
      }
    }
  }
}
